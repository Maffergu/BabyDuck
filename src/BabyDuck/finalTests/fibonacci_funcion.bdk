program fibonacci_funcion;
var n : int;

void fibonacci(ID : int) [
    var i, a, b, temp : int;

    {
        a = 0;
        b = 1;
        i = 0;

        while (i < ID) do {
            print(a);
            temp = a + b;
            a = b;
            b = temp;
            i = i + 1;
        };
    }
];

main {
    n = 10;
    fibonacci(n);
}
end